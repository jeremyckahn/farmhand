{
  "name": "@jeremyckahn/farmhand",
  "version": "1.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A farming game",
  "unpkg": "build/index.html",
  "homepage": "http://jeremyckahn.github.io/farmhand/",
  "scripts": {
    "build": "react-scripts build",
    "build:analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "deploy": "gh-pages -d build",
    "doc": "jsdoc -c .jsdoc -r src",
    "doc:view": "serve -S dist/doc",
    "doc:watch": "nodemon --exec \"npm run doc\" --watch src --watch ./ --ext js,md --ignore dist",
    "lint": "eslint src --max-warnings=0",
    "predeploy": "npm run build",
    "prettier": "prettier 'src/**/*.js' --write",
    "preversion": "CI=true npm run test",
    "postversion": "git push && git push --tags",
    "start": "react-scripts start",
    "start:with-api": "vercel dev",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "update-images": "bash update-images.sh"
  },
  "author": "Jeremy Kahn <jeremyckahn@gmail.com>",
  "license": "CC BY-NC-SA 4.0",
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@jeremyckahn/minami": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^7.13.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "jest-extended": "^0.11.5",
    "jsdoc": "^3.5.5",
    "jsdom": "^16.2.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.3",
    "piskel-cli": "^1.0.41",
    "prettier": "^1.13.7",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^4.0.0",
    "serve": "^11.3.2",
    "vercel": "^21.2.3",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@vercel/node": "^1.9.0",
    "animate.css": "^4.1.0",
    "classnames": "^2.2.6",
    "compass-mixins": "^0.12.10",
    "dinero.js": "^1.7.0",
    "fast-memoize": "^2.5.2",
    "file-saver": "^2.0.2",
    "global": "^4.4.0",
    "localforage": "^1.7.3",
    "lodash.debounce": "^4.0.8",
    "lodash.sortby": "^4.7.0",
    "prop-types": "^15.6.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-file-reader-input": "^2.0.0",
    "react-hotkeys": "^2.0.0",
    "react-markdown": "^4.3.1",
    "react-number-format": "^4.4.1",
    "react-zoom-pan-pinch": "^1.6.1",
    "redis": "^3.0.2",
    "shifty": "^2.15.2",
    "source-map-explorer": "^2.3.1",
    "typeface-francois-one": "0.0.71",
    "typeface-public-sans": "^1.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && cross-env CI=true react-scripts test --env=jsdom --lastCommit"
    }
  },
  "files": [
    "src",
    "build",
    "public"
  ],
  "craGenerate": {
    "cssExtension": "sass",
    "type": "functional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
